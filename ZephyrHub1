-- Scripts:

local function PKLY_fake_script() -- vertical_align_top.LocalScript 
	local script = Instance.new('LocalScript', vertical_align_top)

	local imageLabel = script.Parent
	
	local RunService = game:GetService("RunService")
	
	local function onRenderStep()
		imageLabel.ImageTransparency = math.sin(workspace.DistributedGameTime*math.pi)*.5+.5
	end
	RunService.RenderStepped:Connect(onRenderStep)
end
coroutine.wrap(PKLY_fake_script)()
local function FKNHD_fake_script() -- Loading.LocalScript 
	local script = Instance.new('LocalScript', Loading)

	script.Parent:TweenSize(UDim2.new(0,275,0,1),"InOut","Sine",8)
end
coroutine.wrap(FKNHD_fake_script)()
local function XPLB_fake_script() -- Info_2.LocalScript 
	local script = Instance.new('LocalScript', Info_2)

	-- Made by FrontMan#9917
	script.Parent.Text = "Loading.." -- change text if you whant
	wait(1.3)
	script.Parent.Text = "Checking game.."  -- change text if you whant
	wait(1.3)
	script.Parent.Text = "Getting data.."  -- change text if you whant
	wait(1.3)
	script.Parent.Text = "Bypassing anticheat.."  -- change text if you whant
	wait(2)
	script.Parent.Text = "Executing script.."  -- change text if you whant
	wait(1.3)
end
coroutine.wrap(XPLB_fake_script)()-- Made by FrontMan#9917
wait(9)
Main:Destroy()-- Made by FrontMan#9917

