if game.PlaceId == 2619187362 then

    
    Config = {
        Toggles = {
            autoStrength = false;
            autoPushup = false;
            autoPsych = false;
            autoFuse = false;
            autoRank = false; 
        }
  }



  spawn(function()
    while true do 
        wait(0.1)
        if Config.Toggles.autoRank == true then
        game:GetService("ReplicatedStorage").Functions.BuyRank:InvokeServer()
        end
    end
end)
            
            
    spawn(function()
        while true do 
            wait(0.1)
            if Config.Toggles.autoFuse == true then
            game:GetService("ReplicatedStorage").Functions.BuyFusion:InvokeServer()
            end
        end
    end)

    spawn(function()
        while true do
            wait(0.1)
            if Config.Toggles.autoStrength == true then
                local args = {[1] = "Strength"}
                game:GetService("ReplicatedStorage").Events.Train:FireServer(unpack(args))
            end
        end
    end)


    spawn(function()
        while true do
            wait(0.1)
            if Config.Toggles.autoPushup == true then
                for _,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                    if v:FindFirstChild("PushUp") then
                        game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
                        -- break
                    end
                end
                local args = {[1] = "Endurance"}
                game:GetService("ReplicatedStorage").Events.Train:FireServer(unpack(args))
                wait()
            end
        end
    end)

    spawn(function()
        while true do
            wait(0.1)
            if Config.Toggles.autoPsych == true then
                for _,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                    if v:FindFirstChild("Psychic") then
                        game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
                        -- break
                    end
                end
                local args = {[1] = "Psychic"}
                game:GetService("ReplicatedStorage").Events.Train:FireServer(unpack(args))
                wait()
            end
        end
    end)

